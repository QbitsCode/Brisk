{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": { 
        "buildCommand": "npm run vercel-build",
        "eslint": {
          "enabled": false
        },
        "typescript": {
          "ignoreBuildErrors": true
        }
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ]
}
